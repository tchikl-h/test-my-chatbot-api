{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "target": "es6",
    "module": "commonjs",
    "lib": ["es6","dom"],
    "sourceMap": true,
    "outDir": "built",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
